# ─── Required ───────────────────────────────────────────────
OPENROUTER_API_KEY=           # OpenRouter API key (openrouter.ai)
DISCORD_BOT_TOKEN=            # Discord bot token (discord.com/developers)
TELEGRAM_BOT_TOKEN=           # Telegram bot token from @BotFather
OPENCLAW_GATEWAY_TOKEN=       # Gateway auth token (generate: openssl rand -hex 32)
GH_TOKEN=                     # GitHub PAT scoped to iota-uz org

# ─── Recommended ────────────────────────────────────────────
AUTH_USERNAME=                 # WebUI login username
AUTH_PASSWORD=                 # WebUI login password

# ─── Claude Code ──────────────────────────────────────────
CLAUDE_CODE_OAUTH_TOKEN=      # OAuth token from Claude Max subscription

