{
  "$schema": "https://openclaw.ai/schema/config.json",
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "auth": {
      "mode": "password",
      "password": "${OPENCLAW_GATEWAY_PASSWORD}"
    },
    "controlUi": {
      "enabled": true
    }
  },
  "env": {
    "vars": {
      "OPENROUTER_API_KEY": "${OPENROUTER_API_KEY}"
    }
  },
  "channels": {
    "discord": {
      "enabled": true,
      "token": "${DISCORD_BOT_TOKEN}",
      "dm": {
        "enabled": true,
        "allowFrom": [
          "YOUR_DISCORD_USER_ID_1",
          "YOUR_DISCORD_USER_ID_2"
        ]
      },
      "guilds": {
        "YOUR_DISCORD_GUILD_ID": {
          "slug": "iota",
          "requireMention": true,
          "channels": {}
        }
      }
    },
    "telegram": {
      "enabled": true,
      "botToken": "${TELEGRAM_BOT_TOKEN}",
      "dmPolicy": "allowlist",
      "allowFrom": [
        "YOUR_TELEGRAM_USER_ID_1",
        "YOUR_TELEGRAM_USER_ID_2"
      ],
      "groups": {
        "YOUR_TELEGRAM_GROUP_CHAT_ID": {
          "requireMention": true
        }
      }
    }
  },
  "agents": {
    "defaults": {
      "workspace": "~/.openclaw/workspace",
      "model": {
        "primary": "openrouter/auto"
      }
    },
    "list": [
      {
        "id": "main",
        "default": true,
        "name": "Tolib"
      }
    ]
  }
}
